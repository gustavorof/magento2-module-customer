<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="1column" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <link src="Danke_Company::js/cpfcnpj.js"/>
    </head>
    <body>
        <referenceBlock name="customer_form_register">
            <action method="setTemplate">
                <argument name="template" xsi:type="string">Danke_Company::customer/form/register.phtml</argument>
            </action>
            <container name="form.additional.info" as="form_additional_info">
                <block class="Magento\Framework\View\Element\Template" name="form_additional_info_privacy_policy" after="-" as="privacy_policy" template="Danke_Company::customer/form/register/privacy-policy.phtml"/>
            </container>
        </referenceBlock>
    </body>
</page>
